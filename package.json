{
  "name": "ai-room-collaborator",
  "version": "1.0.0",
  "description": "A real-time collaborative platform with AI assistance",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && uvicorn main:app --reload",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "start": "cd frontend && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VeeramRoshanReddy/AI_Room_Collaborator.git"
  },
  "keywords": [
    "react",
    "fastapi",
    "collaboration",
    "ai",
    "real-time",
    "websocket"
  ],
  "author": "Veeram Roshan Reddy",
  "license": "MIT"
} 